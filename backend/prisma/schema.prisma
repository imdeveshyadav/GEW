generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Listing {
  id        Int      @id @default(autoincrement())
  title     String
  location  String
  price     Int
  images    String[] // array of image paths
  approved  Boolean  @default(false)
  createdAt DateTime @default(now())
}
